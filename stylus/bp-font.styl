@charset "UTF-8";

@font-face {
  font-family: "bigpanda";
  src:url("../fonts/bigpanda.eot");
  src:url("../fonts/bigpanda.eot?#iefix") format("embedded-opentype"),
    url("../fonts/bigpanda.woff") format("woff"),
    url("../fonts/bigpanda.ttf") format("truetype"),
    url("../fonts/bigpanda.svg#bigpanda") format("svg");
  font-weight: normal;
  font-style: normal;

}

[data-icon]:before {
  font-family: "bigpanda" !important;
  content: attr(data-icon);
  font-style: normal !important;
  font-weight: normal !important;
  font-variant: normal !important;
  text-transform: none !important;
  speak: none;
  line-height: 1;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

[class^="bp-icon-"]:before,
[class*=" bp-icon-"]:before {
  font-family: "bigpanda" !important;
